<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="view_order_form_inherit" model="ir.ui.view">
            <field name="name">sale.order.inherited</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_order_form"/>
            <field name="arch" type="xml">
                <field name="partner_id" position="after">
                    <field name="PhoneNumber"/>
                </field>
<!-- add state disable-->
				<header>
                     <button id = "button_disable" name = "action_disable" string = "Disable" class="btn_primary"
                          type = "object" attrs="{'invisible': ['|', ('state', 'not in', ['draft', 'sent','sale']), ('id', '=', False)]}"/>
                </header>
            </field>
        </record>

        <record id="sale.menu_sale_order" model="ir.ui.menu">
            <field name="name">New Register</field>
            <field name="sequence">100</field>
       </record>

        <record id="product.product_normal_form_view" model="ir.ui.menu">
            <field name="name">Exams</field>
            <field name="sequence">100</field>
       </record>

    </data>
</odoo>